@prefix ex: <http://example.org/iadopt/HFR> .
@prefix iop: <https://w3id.org/iadopt/ont/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

ex:HCSS a iop:Variable ;
    rdfs:label "Radial variance of current velocity over coverage period" ;
    rdfs:comment "Variance of multiple measurements taken in a specified time interval (temporal integration time) of the radial component of the velocity of the sea water current on the sea surface." ;
    ex:issue "https://github.com/i-adopt/OMSmodelling/issues/1" ;
    iop:hasMatrix <http://vocab.nerc.ac.uk/collection/S29/current/PE001307/> ;
	iop:hasStatisticalModifier <http://purl.obolibrary.org/obo/STATO_0000113> ;
    iop:hasObjectOfInterest <http://vocab.nerc.ac.uk/collection/S29/current/PE001307/> ;
    iop:hasProperty <https://vocab.nerc.ac.uk/collection/S06/current/S0600265/> ;
	iop:hasContextObject <http://opendata.inrae.fr/thesaurusINRAE/c_1384> ;
	iop:hasConstraint [a iop:Constraint ;
                       rdfs:label "over a specific time period" ;
                       iop:constrains <http://purl.obolibrary.org/obo/STATO_0000113> ] . 				   
	   
	

<http://vocab.nerc.ac.uk/collection/S29/current/PE001307/> a iop:Entity ;
    rdfs:label "surface water" .

<http://vocab.nerc.ac.uk/collection/S29/current/PE001307/> a iop:Entity ;
    rdfs:label "water current" .
	
<https://vocab.nerc.ac.uk/collection/S06/current/S0600265/> a iop:Property ;
    rdfs:label "radial velocity" .
	
<http://opendata.inrae.fr/thesaurusINRAE/c_1384> a iop:ContextObject ;
    rdfs:label "sea" .
	
<http://purl.obolibrary.org/obo/STATO_0000113> a iop:StatisticalModifier ;
    rdfs:label "variance" .
	