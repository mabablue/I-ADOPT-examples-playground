ex:Matrix
    a 
        iop:Entity ;
    rdfs:label 
        "water body" .

<http://purl.obolibrary.org/obo/STATO_0000113>
    a 
        iop:StatisticalModifier ;
    rdfs:label 
        "variance" .

ex:Prop
    a 
        iop:Property ;
    rdfs:label 
        "velocity" .

