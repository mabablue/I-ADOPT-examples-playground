@prefix iop: <https://w3id.org/iadopt/ont/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix ex: <http://example.org/iadopt/challenge/> .

ex:someVar
    a 
        iop:Variable ;
    rdfs:label 
        "HCSS Radial velocity variance of water current relative to instrument  in the water body by high frequency radar" ;
    rdfs:comment 
        """HCSS (m2 s-2) radial data file""" ;
    iop:hasObjectOfInterest 
        <http://vocab.nerc.ac.uk/collection/S29/current/PE001307/> ;
    iop:hasMatrix 
        ex:Matrix ;
    iop:hasContextObject 
        <http://opendata.inrae.fr/thesaurusINRAE/c_1384> ;
    iop:hasStatisticalModifier 
        <http://purl.obolibrary.org/obo/STATO_0000113> ;
    iop:hasProperty 
        ex:Prop ;
    iop:hasConstraint 
        [ a iop:Constraint ;
             rdfs:label "radial relative to instrument" ;
             iop:constrains ex:Prop ;
        ] ,
        [ a iop:Constraint ;
             rdfs:label "at the surface" ;
             iop:constrains ex:Matrix ;
        ] ,
        [ a iop:Constraint ;
             rdfs:label "over temporal coverage period" ;
             iop:constrains <http://purl.obolibrary.org/obo/STATO_0000113> ;
        ] .

<http://vocab.nerc.ac.uk/collection/S29/current/PE001307/>
    a 
