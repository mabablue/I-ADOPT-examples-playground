ex:someVar
    a 
        iop:Variable ;
    rdfs:label 
        "VACC Northward velocity uncertainty of water current in the water body by high frequency radar" ;
    rdfs:comment 
        """VACC (m s-1)""" ;
    iop:hasObjectOfInterest 
        <http://vocab.nerc.ac.uk/collection/S29/current/PE001307/> ;
    iop:hasMatrix 
        ex:Matrix ;
    iop:hasContextObject 
        <http://opendata.inrae.fr/thesaurusINRAE/c_1384> ;
    iop:hasStatisticalModifier 
        _:b0 ;
    iop:hasProperty 
        ex:Prop ;
    iop:hasConstraint 
        [ a iop:Constraint ;
             rdfs:label "at the surface" ;
             iop:constrains ex:Matrix ;
        ] ,
        [ a iop:Constraint ;
             rdfs:label "northward" ;
             iop:constrains ex:Prop ;
        ] .

