@prefix iop: <https://w3id.org/iadopt/ont/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix ex: <http://example.org/iadopt/challenge/> .

ex:someVar
    a 
        iop:Variable ;
    rdfs:label 
        "Radial velocity of water current relative to instrument and True North in the water body by high frequency radar" ;
    rdfs:comment 
        """RDVA (m s-1) radial data file""" ;
    iop:hasObjectOfInterest 
        <http://vocab.nerc.ac.uk/collection/S29/current/PE001307/> ;
    iop:hasMatrix 
        ex:Matrix ;
    iop:hasContextObject 
        <http://opendata.inrae.fr/thesaurusINRAE/c_1384> ;
    iop:hasProperty 
        ex:Prop ;
    iop:hasConstraint 
        [ a iop:Constraint ;
             rdfs:label "radial" ;
             iop:constrains ex:Prop ;
        ] ,
        [ a iop:Constraint ;
             rdfs:label "surface layer" ;
             iop:constrains ex:Matrix ;
        ] ,
        [ a iop:Constraint ;
             rdfs:label "relative to instrument" ;
             iop:constrains ex:Prop ;
        ] ,
        [ a iop:Constraint ;
             rdfs:label "relative to True North" ;
             iop:constrains <http://vocab.nerc.ac.uk/collection/S29/current/PE001307/> ;
        ] .

<http://vocab.nerc.ac.uk/collection/S29/current/PE001307/>
    a 
        iop:Entity ;
    rdfs:label 
        "water current" .

ex:Matrix
    a 
        iop:Entity ;
    rdfs:label 
        "water" .

ex:Prop
    a 
        iop:Property ;
    rdfs:label 
        "velocity" .

<http://opendata.inrae.fr/thesaurusINRAE/c_1384>
    a 
        iop:Entity ;
    rdfs:label 
        "sea" .
