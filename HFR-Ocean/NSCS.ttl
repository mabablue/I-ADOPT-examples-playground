@prefix ex: <http://example.org/iadopt/HFR> .
@prefix iop: <https://w3id.org/iadopt/ont/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

ex:NSCS a iop:Variable ;
    rdfs:label "Standard deviation of surface northward sea water velocity" ;
    rdfs:comment "Standard deviation of multiple measurements taken in a specified time interval (temporal integration time) of the South-to-North component of the velocity of the sea water current on the sea surface" ;
    ex:issue "https://github.com/i-adopt/OMSmodelling/issues/1" ;
    iop:hasMatrix <http://vocabs.lter-europe.net/EnvThes/22225> ;
    iop:hasObjectOfInterest <http://vocab.nerc.ac.uk/collection/S29/current/PE001307/> ;
	iop:hasStatisticalModifier <http://www.ontology-of-units-of-measure.org/resource/om-2/standardDeviation> ;
    iop:hasProperty <http://qudt.org/vocab/quantitykind/Velocity> ;
	iop:hasContextObject <http://opendata.inrae.fr/thesaurusINRAE/c_1384> ;
	iop:hasConstraint [a iop:Constraint ;
                       rdfs:label "northward" ;
                       iop:constrains <http://qudt.org/vocab/quantitykind/Velocity> ] . 				   
	

<http://vocabs.lter-europe.net/EnvThes/22225> a iop:Entity ;
    rdfs:label "surface water" .

<http://vocab.nerc.ac.uk/collection/S29/current/PE001307/> a iop:Entity ;
    rdfs:label "water current" .
	
<http://qudt.org/vocab/quantitykind/Velocity> a iop:Property ;
    rdfs:label "velocity" .
	
<http://opendata.inrae.fr/thesaurusINRAE/c_1384> a iop:ContextObject ;
    rdfs:label "sea" .
	
<http://www.ontology-of-units-of-measure.org/resource/om-2/standardDeviation> a iop:StatisticalModifier ;
    rdfs:label "standard deviation" . 
	
	



