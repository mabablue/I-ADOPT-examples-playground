ex:RadVelDirHF a iop:Variable ;
    rdfs:label "Direction of radial vector relative to instrument and True North in the water body by high frequency radar" ;
    rdfs:comment "The compass direction in degrees positive clockwise from True North of the radial component of water velocity in a water body by high frequency radar observation." ;
    ex:issue "https://github.com/i-adopt/OMSmodelling/issues/1" ;
    iop:hasMatrix <http://vocabs.lter-europe.net/EnvThes/22225> ;
    iop:hasObjectOfInterest ex:System ;
        iop:hasProperty <http://qudt.org/vocab/quantitykind/Angle> ;
	iop:hasContextObject <http://opendata.inrae.fr/thesaurusINRAE/c_1384> ;
    iop:hasConstraint [ a iop:Constraint ;
                  rdfs:label "relative to instrument" ;
                  iop:constrains ex:System ] . 
	
