    rdfs:label "water current" .
	
<http://purl.obolibrary.org/obo/ERO_0000004> a iop:Entity ;
    rdfs:label "instrument" .
	
<http://qudt.org/vocab/quantitykind/Angle> a iop:Property ;
    rdfs:label "angle" .
	
<http://opendata.inrae.fr/thesaurusINRAE/c_1384> a iop:ContextObject ;
    rdfs:label "sea" .
		
<http://purl.obolibrary.org/obo/PATO_0045091> a iop:Entity ;
    rdfs:label "north" .

<http://vocabs.lter-europe.net/EnvThes/22225> a iop:Entity ;
   rdfs:label "sea surface" .

