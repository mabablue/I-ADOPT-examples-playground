@prefix iop: <https://w3id.org/iadopt/ont/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix ex: <http://example.org/iadopt/challenge/> .

ex:someVar
    a 
        iop:Variable ;
    rdfs:label 
        "Eastward velocity standard deviation water current" ;
    rdfs:comment 
        """EWCS (m s-1) total data file""" ;
    iop:hasObjectOfInterest 
        ex:OoI ;
    iop:hasMatrix 
        ex:Matrix ;
    iop:hasContextObject 
        ex:CO ;
    iop:hasStatisticalModifier 
        ex:StatMod ;
    iop:hasProperty 
        ex:Prop ;
    iop:hasConstraint 
        [ a iop:Constraint ;
             rdfs:label "eastward" ;
             iop:constrains ex:Prop ;
        ] .

ex:OoI
    a 
        iop:Entity ;
    rdfs:label 
        "Water current" .

ex:Matrix
    a 
        iop:Entity ;
    rdfs:label 
        "water" .

ex:StatMod
    a 
        iop:StatisticalModifier ;
    rdfs:label 
        "standard deviation" .

ex:Prop
    a 
        iop:Property ;
    rdfs:label 
        "Velocity" .

ex:CO
    a 
        iop:Entity ;
    rdfs:label 
        "sea" .
