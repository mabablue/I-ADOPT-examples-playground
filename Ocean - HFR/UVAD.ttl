ex:someVar
    a 
        iop:Variable ;
    rdfs:label 
        "UVAD Eastward velocity average in the water by a moored acoustic doppler current profile (ADCP)" ;
    rdfs:comment 
        """UVAD (m s-1) - ADCP datafile""" ;
    iop:hasObjectOfInterest 
        <http://vocab.nerc.ac.uk/collection/S29/current/PE001307/> ;
    iop:hasMatrix 
        <http://vocab.nerc.ac.uk/collection/S21/current/S21S027/> ;
    iop:hasContextObject 
        <http://opendata.inrae.fr/thesaurusINRAE/c_1384> ;
    iop:hasStatisticalModifier 
        <http://purl.obolibrary.org/obo/STATO_0000573> ;
    iop:hasProperty 
        <http://qudt.org/vocab/quantitykind/Velocity> ;
    iop:hasConstraint 
        [ a iop:Constraint ;
             rdfs:label "eastward" ;
             iop:constrains <http://qudt.org/vocab/quantitykind/Velocity> ;
        ] ,
        [ a iop:Constraint ;
             rdfs:label "eulerian" ;
             iop:constrains <http://qudt.org/vocab/quantitykind/Velocity> ;
        ] .

