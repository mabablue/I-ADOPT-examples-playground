        iop:Variable ;
    rdfs:label 
        "Sea surface temperature" ;
    rdfs:comment 
        """Radiative skin sea surface temperature, or Bulk sea surface temperature at stated depth""" ;
    iop:hasObjectOfInterest 
        <http://vocabs.lter-europe.net/EnvThes/22225> ;
    iop:hasContextObject 
        <http://purl.obolibrary.org/obo/ENVO_00000016> ;
    iop:hasProperty 
        <http://qudt.org/vocab/quantitykind/ThermodynamicTemperature> ;
    iop:hasConstraint 
        [ a iop:Constraint ;
             rdfs:label "location: at the surface" ;
             iop:constrains <http://vocabs.lter-europe.net/EnvThes/22225> ;
        ] .

