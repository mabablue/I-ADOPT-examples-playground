@prefix ex: <http://example.org/iadopt/challenge/> .
@prefix iop: <https://w3id.org/iadopt/ont/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

ex:slvar a iop:Variable ;
    rdfs:label "solubility of naphthalene in water at room temperature" ;
    rdfs:comment "solubility measurement of napathalene in deionized water at room temperature by monitoring absorption at 279 nm" ;
    iop:hasConstraint [
      a iop:Constraint ;
      rdfs:label "solid phase" ;
      iop:constrains ex:SysSource
    ], [
      a iop:Constraint ;
      rdfs:label "liquid phase" ;
      iop:constrains ex:SysTarget
    ], [
      a iop:Constraint ;
      rdfs:label "equilibrium mixture of a solid and a liquid" ;
      iop:constrains ex:OoI
    ], [
      a iop:Constraint ;
      rdfs:label "local room temperature" ;
      iop:constrains ex:Prop
    ], [
      a iop:Constraint ;
      rdfs:label "monitoring at 279 nm" ;
      iop:constrains ex:Prop
    ], [
      a iop:Constraint ;
      rdfs:label "type: mass/volume" ;
      iop:constrains ex:Prop
    ] ;
    iop:hasMatrix ex:Matrix ;
    iop:hasObjectOfInterest ex:OoI ;
    iop:hasProperty ex:Prop .

ex:Matrix
    a iop:Entity, iop:System ;
    rdfs:label "two-phase solid-liquid system" ;
    iop:hasSource ex:SysSource;
    iop:hasTarget ex:SysTarget.

ex:OoI
    a iop:Entity ;
    rdfs:label "solubilized napthalene" .

ex:Prop
    a iop:Property ;
    rdfs:label "solubility" .

ex:SysSource
    a iop:Entity ;
    rdfs:label "naphthalene (C10H8)" .

ex:SysTarget
    a iop:Entity ;
    rdfs:label "water (H2O)" .