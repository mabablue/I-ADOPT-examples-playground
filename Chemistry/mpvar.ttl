@prefix ex: <http://example.org/iadopt/challenge/> .
@prefix iop: <https://w3id.org/iadopt/ont/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

ex:mpvar a iop:Variable ;
    rdfs:label "melting point of naphthalene under standard conditions" ;
    rdfs:comment "measurement of the thermodynamic temperature (or temperature range) for the transformation of a known purity (99.12% (w/w)) solid sample of naphthalene into a liquid" ;
    iop:hasConstraint [
      a iop:Constraint ;
      rdfs:label "at the solid to liquid phase transition" ;
      iop:constrains ex:Prop
    ], [
      a iop:Constraint ;
      rdfs:label "purity of 99.12% (w/w)" ;
      iop:constrains ex:OoI
    ] ;
    iop:hasObjectOfInterest ex:OoI ;
    iop:hasProperty ex:Prop .

ex:OoI
    a iop:Entity ;
    rdfs:label "naphthalene (C10H8)" .

ex:Prop
    a iop:Property ;
    rdfs:label "thermodynamic temperature" .