@prefix ex: <http://example.org/iadopt/challenge/> .
@prefix iop: <https://w3id.org/iadopt/ont/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

ex:C4 a iop:Variable ;
    rdfs:label "Atmospheric boundary layer height defined by temperature inversion" ;
    rdfs:comment "Atmospheric boundary layer height defined by temperature inversion." ;
    ex:issue "https://github.com/i-adopt/examples/issues/21" ;
    iop:hasConstraint    [ a iop:Constraint ;
            rdfs:label "due to temperature inversion" ;
            iop:constrains <http://vocabs.lter-europe.net/EnvThes/21543>],
                   [ a iop:Constraint ;
            rdfs:label "above the valley floor" ;
            iop:constrains <http://qudt.org/vocab/quantitykind/Height> ] ;
    iop:hasMatrix <http://vocabs.lter-europe.net/EnvThes/21920> ;
    iop:hasObjectOfInterest <http://vocabs.lter-europe.net/EnvThes/21543> ;
    iop:hasProperty <http://qudt.org/vocab/quantitykind/Height> .

<http://vocabs.lter-europe.net/EnvThes/21920> a iop:Entity ;
    rdfs:label "atmosphere" .

<http://vocabs.lter-europe.net/EnvThes/21543> a iop:Entity ;
    rdfs:label "atmospheric boundary layer" .

<http://qudt.org/vocab/quantitykind/Height> a iop:Property ;
    rdfs:label "height" .

