@prefix ex: <http://example.org/iadopt/challenge/> .
@prefix iop: <https://w3id.org/iadopt/ont/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

ex:C11 a iop:Variable ;
    rdfs:label "Strike of bedding" ;
    rdfs:comment "Geographic azimuth (relative to true north) at a point observation location, of a horizontal line contained in a sedimentary rock bedding surface. The bedding surface must not be horizontal; the azimuth is reported such that the dip direction of the inclined bedding is to the right when facing in the azimuth direction." ;
    ex:issue "https://github.com/i-adopt/examples/issues/28" ;
    iop:hasMatrix <http://opendata.inrae.fr/thesaurusINRAE/c_5217> ;
    iop:hasObjectOfInterest [
     a iop:System, iop:SymmetricSystem ;
    rdfs:label "line of intersection" ;
    iop:hasPart <http://data.bgs.ac.uk/id/GeoscienceThesaurus/Concept/428>, <https://www.wikidata.org/wiki/Q1055798> ] ;
    iop:hasProperty <http://qudt.org/vocab/quantitykind/Azimuth> .

<http://opendata.inrae.fr/thesaurusINRAE/c_5217> a iop:Entity ;
    rdfs:label "sedimentary rock" .

<http://data.bgs.ac.uk/id/GeoscienceThesaurus/Concept/428> a iop:Entity ;
    rdfs:label "bedding plane" .

<https://www.wikidata.org/wiki/Q1055798> a iop:Entity ;
    rdfs:label "horizontal plane" .

<http://qudt.org/vocab/quantitykind/Azimuth> a iop:Property ;
    rdfs:label "azimuth" .

