@prefix ex: <http://example.org/iadopt/challenge/> .
@prefix iop: <https://w3id.org/iadopt/ont/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

ex:C22 a iop:Variable ;
    rdfs:label "Current smoking status" ;
    rdfs:comment "Smoking status current of a patient" ;
    ex:issue "https://github.com/i-adopt/examples/issues/39" ;
    iop:hasConstraint [ a iop:Constraint ;
            rdfs:label "current" ;
            iop:constrains <http://purl.obolibrary.org/obo/NCIT_C19796> ] ;
    iop:hasObjectOfInterest <http://purl.obolibrary.org/obo/NCIT_C16960> ;
    iop:hasProperty <http://purl.obolibrary.org/obo/NCIT_C19796> .

<http://purl.obolibrary.org/obo/NCIT_C16960> a iop:Entity ;
    rdfs:label "patient" .

<http://purl.obolibrary.org/obo/NCIT_C19796> a iop:Property ;
    rdfs:label "smoking status" .