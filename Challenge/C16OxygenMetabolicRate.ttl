@prefix ex: <http://example.org/iadopt/challenge/> .
@prefix iop: <https://w3id.org/iadopt/ont/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

ex:C16 a iop:Variable ;
    rdfs:label "Standard metabolic rate in mg of Oxygen per hour" ;
    rdfs:comment "The measure of an ectotherm animal's baseline metabolic rate (i.e. non-active, non-stressed) measured by the milligrams of oxygen consumed in a period of time of one hour." ;
    ex:issue "https://github.com/i-adopt/examples/issues/33" ;
    iop:hasConstraint [ a iop:Constraint ;
            rdfs:label "ectotherm" ;
            iop:constrains <http://vocabs.lter-europe.net/EnvThes/26> ],
        [ a iop:Constraint ;
            rdfs:label "due to metabolism" ;
            iop:constrains <http://qudt.org/vocab/quantitykind/MassFlowRate> ],
        [ a iop:Constraint ;
            rdfs:label "non-stressed" ;
            iop:constrains <http://vocabs.lter-europe.net/EnvThes/26> ],
        [ a iop:Constraint ;
            rdfs:label "non-active" ;
            iop:constrains <http://vocabs.lter-europe.net/EnvThes/26> ] ;
    iop:hasMatrix [
      a iop:System, iop:AsymmetricSystem ;
		  rdfs:label "from air to organism" ;
		  iop:hasSource <http://vocabs.lter-europe.net/EnvThes/23> ;
		  iop:hasTarget <http://vocabs.lter-europe.net/EnvThes/26>
    ] ;
    iop:hasObjectOfInterest <http://vocabs.lter-europe.net/EnvThes/20850> ;
    iop:hasProperty <http://qudt.org/vocab/quantitykind/MassFlowRate> .

<http://vocabs.lter-europe.net/EnvThes/20850> a iop:Entity ;
    rdfs:label "oxygen" .
	
<http://vocabs.lter-europe.net/EnvThes/23> a iop:Entity ;
    rdfs:label "air" . 

<http://vocabs.lter-europe.net/EnvThes/26> a iop:Entity ;
    rdfs:label "organism" . 
