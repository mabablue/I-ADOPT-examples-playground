@prefix ex: <http://example.org/iadopt/challenge/> .
@prefix iop: <https://w3id.org/iadopt/ont/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

ex:C9 a iop:Variable ;
    rdfs:label "Foliage projective cover in the lower canopy strata" ;
    rdfs:comment "The proportion of the ground area covered by foliage (or photosynthetic tissue) held in a vertical plane. Foliage Projective Cover is usually expressed as the percentage of ground covered by foliage and can be separated according to vegetation strata." ;
    ex:issue "https://github.com/i-adopt/examples/issues/26" ;
    iop:hasConstraint [ a iop:Constraint ;
            rdfs:label "lower canopy strata" ;
            iop:constrains <http://vocabs.lter-europe.net/EnvThes/20575> ] ;
    iop:hasMatrix <http://vocabs.lter-europe.net/EnvThes/30365> ;
    iop:hasObjectOfInterest <http://vocabs.lter-europe.net/EnvThes/20575>;
    iop:hasProperty <http://www.ontology-of-units-of-measure.org/resource/om-2/AreaFraction> .

<http://vocabs.lter-europe.net/EnvThes/30365> a iop:Entity ;
    rdfs:label "ground" .

<http://www.ontology-of-units-of-measure.org/resource/om-2/AreaFraction> a iop:Property ;
    rdfs:label "area fraction" .

<http://vocabs.lter-europe.net/EnvThes/20575> a iop:Entity ;
    rdfs:label "foliage" .

