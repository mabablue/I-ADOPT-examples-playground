@prefix ex: <http://example.org/iadopt/challenge/> .
@prefix iop: <https://w3id.org/iadopt/ont/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

ex:C14 a iop:Variable ;
    rdfs:label "Feral-free enclosure area" ;
    rdfs:comment "The total area (in ha) of feral-free enclosures. See https://linked.data.gov.au/def/nrm/d936bfd5-d2f9-5107-bbf7-2bd18bbab42b" ;
    ex:issue "https://github.com/i-adopt/examples/issues/31" ;
    iop:hasConstraint [ a iop:Constraint ;
            rdfs:label "free of feral" ;
            iop:constrains <http://opendata.inrae.fr/thesaurusINRAE/c_1152> ] ;
    iop:hasObjectOfInterest <http://opendata.inrae.fr/thesaurusINRAE/c_1152> ;
    iop:hasProperty <http://qudt.org/vocab/quantitykind/Area> .

<http://qudt.org/vocab/quantitykind/Area> a iop:Property ;
    rdfs:label "area" .

<http://opendata.inrae.fr/thesaurusINRAE/c_1152> a iop:Entity ;
    rdfs:label "enclosure" .

